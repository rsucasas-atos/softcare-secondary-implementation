<UserControl x:Class="EHealth.ClientApplication.Controls.MediaContentControl"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:local="clr-namespace:EHealth.ClientApplication"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d">
    <StackPanel Name="Task1" Orientation="Horizontal">
        <Button Margin="10,10,10,20" Command="local:AppCommands.MoveToPageCommand" CommandParameter="{Binding MediaContent}" Style="{StaticResource TaskButton}" Content="{Binding Title}"></Button>
    </StackPanel>
</UserControl>

