<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eu.ehealth.db.db">
 <class name="EntertainmentContent">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="identity"/>
  </id>
  <property generated="never" lazy="false" name="title" type="string">
   <column name="title"/>
  </property>
  <property generated="never" lazy="false" name="url" type="string">
   <column name="url"/>
  </property>
  <property generated="never" lazy="false" name="type" type="string">
   <column name="type"/>
  </property>
  <property generated="never" lazy="false" name="category" type="string">
   <column name="category"/>
  </property>
  <property generated="never" lazy="false" name="text" type="string">
   <column name="text"/>
  </property>
  <property generated="never" lazy="false" name="enabled" type="java.lang.Boolean">
   <column name="enabled"/>
  </property>
 </class>
</hibernate-mapping>
